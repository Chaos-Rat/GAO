	"version": "0.2.0",
	"configurations": [
		{
			"type": "java",
			"name": "Current File",
			"request": "launch",
			"mainClass": "${file}"
		},
		{
            "type": "java",
            "name": "JavaFX",
            "request": "launch",
            "mainClass": "aste.client.HelloApplication",
            "vmArgs": "--module-path 'JAVAFX_PATH' --add-modules javafx.controls,javafx.fxml"
        },
		{
			"type": "java",
			"name": "Applicazione",
			"request": "launch",
			"mainClass": "aste.server.Applicazione"
		}
	]
}